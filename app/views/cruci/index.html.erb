			var palabra11=new String();			
			for(var i=3;i<=10;i++)
			{
				palabra1=palabra1.concat(tabla.rows[i].cells[8].children[0].value);				
			}				
			for(var i=2;i<=7;i++)
			{
				palabra2=palabra2.concat(tabla.rows[i].cells[13].children[0].value);				
			}	
