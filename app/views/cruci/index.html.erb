			{
				palabra3=palabra3.concat(tabla.rows[i].cells[6].children[0].value);				
			}					
			for(var i=1;i<=6;i++)
			{
				palabra4=palabra4.concat(tabla.rows[i].cells[4].children[0].value);				
			}			
			for(var i=2;i<=6;i++)
			{			
				palabra5=palabra5.concat(tabla.rows[2].cells[i].children[0].value);				
			}
			for(var i=1;i<=4;i++)
			{
				palabra6=palabra6.concat(tabla.rows[6].cells[i].children[0].value);				
			}
			for(var i=5;i<=8;i++)
			{
				palabra7=palabra7.concat(tabla.rows[10].cells[i].children[0].value);				
			}			
			for(var i=4;i<=8;i++)
			{
				palabra8=palabra8.concat(tabla.rows[5].cells[i].children[0].value);				
			}			
			for(var i=8;i<=13;i++)
			{
				palabra9=palabra9.concat(tabla.rows[3].cells[i].children[0].value);				
			}		
			for(var i=13;i<=15;i++)
			{
				palabra10=palabra10.concat(tabla.rows[4].cells[i].children[0].value);				
			}			
			for(var i=12;i<=15;i++)
			{
				palabra11=palabra11.concat(tabla.rows[6].cells[i].children[0].value);				
			}			
			if(palabra1.toLowerCase()=="checkout" && palabra2.toLowerCase()=="status" && palabra3.toLowerCase()=="log" && palabra4.toLowerCase()=="branch"&& palabra5.toLowerCase()=="merge" &&palabra6.toLowerCase()=="push" &&palabra7.toLowerCase()=="init" && palabra8.toLowerCase()=="clone" && palabra9.toLowerCase()=="commit" && palabra10.toLowerCase()=="add" && palabra11.toLowerCase() =="pull")
			 {
